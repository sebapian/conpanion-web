{
  "name": "conpanion-web",
  "version": "0.0.1",
  "description": "Companion Web",
  "author": "Companion",
  "license": "MIT",
  "homepage": "https://conpanion-web.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebapian/conpanion-web"
  },
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "sb": "supabase",
    "types:db": "supabase gen types typescript --local > lib/supabase/types.generated.d.ts",
    "db:dump": "supabase db dump --linked --data-only -f ./supabase/seed.sql",
    "db:reset": "supabase db reset && npm run types:db",
    "db:dr": "npm run db:dump && npm run db:reset",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "latest",
    "next-themes": "^0.4.3",
    "prettier": "^3.3.3",
    "react": "19.0.0",
    "react-day-picker": "^9.6.5",
    "react-dom": "19.0.0",
    "react-textarea-autosize": "^8.5.9",
    "resend": "^4.6.0",
    "sonner": "^2.0.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "19.0.2",
    "eslint": "9.24.0",
    "eslint-config-next": "15.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "postcss": "8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "supabase": "^2.19.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.2"
  },
  "lint-staged": {
    "supabase/migrations/*.sql": "npm run types:db",
    "*": "npm run prettier:fix"
  }
}
